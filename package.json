    "concurrently": "^8.2.2",
    "cssnano": "^6.0.3",
    "dotenv": "^16.4.1",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eleventy-plugin-toc": "^1.1.5",
    "html-minifier": "^4.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-toc-done-right": "^4.2.0",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
