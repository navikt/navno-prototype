---
title: Produktside
---

{% extends "blank.njk" %}

{% block content %}
  <div class="grid content-center w-full min-h-screen gap-4 place-items-center">
    {% for item in collections.produkt %}
      <a class="px-6 py-4 rounded bg-slate-100 hover:bg-blue-500 hover:text-white" href="{{ item.url }}">Variant {{ item.data.variant | safe }}</a>
    {% endfor %}
  </div>
{% endblock %}