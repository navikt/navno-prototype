    <link rel="stylesheet" href="/assets/main.min.css" type="text/css" />
    {%- block styles -%}{%- endblock -%}

    {# <script src="/assets/alpineFocus.min.js" type="text/javascript" defer></script> #}
    <script src="/assets/alpine.min.js" type="text/javascript" defer></script>
